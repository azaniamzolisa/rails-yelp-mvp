<h1>New review</h1>
<%= simple_form_for (@restaurant)do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, as: :select, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back to Restaurants', restaurants_path %>
